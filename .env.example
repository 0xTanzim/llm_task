# Google AI API Key (required)
GOOGLE_API_KEY="your-google-api-key-here"

# LLM Configuration (optional - has defaults)
LLM_MODEL="gemini-2.5-flash"
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=2048

# OpenAI Compatibility Base URL (optional - has default)
OPENAI_BASE_URL="https://generativelanguage.googleapis.com/v1beta/openai/"

# App Settings (optional)
APP_NAME="FastAPI LangChain Service"
DEBUG=false


## Langsmith Configuration : https://smith.langchain.com/
export LANGSMITH_TRACING=true
export LANGSMITH_API_KEY=""
export LANGSMITH_PROJECT=""



## tavily Configuration : https://www.tavily.com/
export TAVILY_API_KEY=""


